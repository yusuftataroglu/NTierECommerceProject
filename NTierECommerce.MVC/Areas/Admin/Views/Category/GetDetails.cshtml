@model Category
@{
    ViewData["Title"] = "GetDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Details</h1>
<div class="row">
    <div class="col-md-auto">
        <table class="table table-hover table-bordered">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Master ID</th>
                    <th>Category Name</th>
                    <th>Description</th>
                    <th>Created Date</th>
                    <th>Created Computer Name</th>
                    <th>Created Ip Address</th>
                    <th>Updated Date</th>
                    <th>Updated Computer Name</th>
                    <th>Updated Ip Address</th>
                    <th>Status</th>
                    <th>Active</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Model.Id</td>
                    <td>@Model.MasterId</td>
                    <td>@Model.CategoryName</td>
                    <td>@Model.Description</td>
                    <td>@Model.CreatedDate</td>
                    <td>@Model.CreatedComputerName</td>
                    <td>@Model.CreatedIpAddress</td>
                    <td>@Model.UpdatedDate</td>
                    <td>@Model.UpdatedComputerName</td>
                    <td>@Model.UpdatedIpAddress</td>
                    <td>@Model.Status</td>
                    <td>@Model.IsActive</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

